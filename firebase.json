{
  "functions": {
    "ignore": [
      ".eslintrc.js",
      ".gitignore",
      "**/src/**",
      "tsconfig.dev.json",
      "tsconfig.json",
      "ui-debug.log",
      "**/node_modules/**"
    ],
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "**/**",
        "function": "server"
      }
    ],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}
